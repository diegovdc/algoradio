{
  "version": "1.3.0",
  "scripts": {
    "publish": "npm version ${npm_config_versiontype} && git push origin master",
    "deps": "yarn install",
    "build-html": "shadow-cljs clj-run build.html.core",
    "watch": "shadow-cljs watch app;",
    "dev:styles": "node-sass src/styles/mazorca.scss public/css/mazorca.css --watch",
    "release": "shadow-cljs release app && parcel build src/js/index.js --out-dir public/js/external-libs && node-sass src/styles/mazorca.scss public/css/mazorca.css && npm run build-html",
    "server": "shadow-cljs server;",
    "clean": "rm -rf target; rm -rf public/js/compiled"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "codemirror": "^5.53.2",
    "create-react-class": "^15.6.2",
    "howler": "^2.1.3",
    "hydra-synth": "^1.3.6",
    "react": "^16.0.0",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.0.0",
    "shadow-cljs": "2.10.19",
    "y-codemirror": "^2.0.9",
    "y-websocket": "^1.3.4",
    "yjs": "^13.4.0"
  },
  "devDependencies": {
    "mazorca-core": "^1.6.2",
    "parcel": "^1.12.4",
    "node-sass": "^4.14.1"
  }
}
